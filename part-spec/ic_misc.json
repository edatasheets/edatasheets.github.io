                },
                "currentConsumption": {
                    "description": "current used by device in various power modes",
                    "$ref": "definitions.json#/currentConsumption"
                },
                "pins": {
                    "description": "array of pin objects with associated properties",
                    "type": "array",
                    "items": {
                        "$ref": "definitions.json#/pinSpec"
                    }
                }
            }
